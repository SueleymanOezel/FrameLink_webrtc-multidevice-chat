{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "app.js", "use": "@vercel/static" },
    { "src": "debug.html", "use": "@vercel/static" },
    { "src": "simple-room.js", "use": "@vercel/static" },
    { "src": "room-video-streaming.js", "use": "@vercel/static" },
    { "src": "face-detection.js", "use": "@vercel/static" },
    { "src": "auto-camera-switching.js", "use": "@vercel/static" },
    { "src": "integration-fix.js", "use": "@vercel/static" },
    { "src": "models/*", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/$1" }
  ]
}